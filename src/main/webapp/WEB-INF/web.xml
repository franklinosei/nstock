<?xml version="1.0" encoding="UTF-8"?>
<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">
    <servlet>
        <servlet-name>dashboard</servlet-name>
        <servlet-class>views.dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stock</servlet-name>
        <servlet-class>views.stock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>item</servlet-name>
        <servlet-class>views.item</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>labs</servlet-name>
        <servlet-class>views.labs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>technician</servlet-name>
        <servlet-class>views.technician</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newLab</servlet-name>
        <servlet-class>views.newLab</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newTechnician</servlet-name>
        <servlet-class>views.newTechnician</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stock</servlet-name>
        <url-pattern>/stock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>item</servlet-name>
        <url-pattern>/item</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>labs</servlet-name>
        <url-pattern>/labs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>technician</servlet-name>
        <url-pattern>/technician</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newLab</servlet-name>
        <url-pattern>/newLab</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newTechnician</servlet-name>
        <url-pattern>/newTechnician</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
